<div id="content">
  <h1> <%= @topic.name %> </h1>

  <ul>
    <% @topic.contents.each do |content| %>
      <li>
        <%= content.content %> <%= link_to "Edit", edit_content_path(content) %> <br><br>
      </li>
    <% end %>
  </ul>

</div>

<div id="link">
  <%= link_to "Return back to Programming Topics", topics_path %>
</div>
